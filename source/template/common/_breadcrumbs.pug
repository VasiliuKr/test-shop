.breadcrumbs
	.container
		ul.breadcrumbs__list
			- var items = ["Главная", "Каталог"]
				each item, index in items
					li(class="breadcrumbs__item breadcrumbs__item_" + ((item == "Каталог") ? 'active' : ''))
						a(href="#").breadcrumbs__link= item
						if index != items.length-1
							span.breadcrumbs__pipe   >  